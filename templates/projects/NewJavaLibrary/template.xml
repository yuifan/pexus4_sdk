<?xml version="1.0"?>
<template
    format="1"
    revision="1"
    name="Java Library"
    description="Creates a new Java library.">

    <thumbs>
        <thumb>template_new_project.png</thumb>
    </thumbs>

    <category value="Applications" />

    <parameter
        id="projectName"
        name="Library name"
        type="string"
        constraints="nonempty"
        default="MyLibrary"/>

    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />

</template>
